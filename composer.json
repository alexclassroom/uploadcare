{
  "name": "uploadcare/uploadcare-wordpress",
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "uploadcare/uploadcare-php": "^3.0",
    "deliciousbrains/wp-background-processing": "^1.0",
    "symfony/dom-crawler": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Uploadcare\\": "vendor/uploadcare/uploadcare-php/"
    }
  },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "symfony/var-dumper": "^4.4"
  }
}
